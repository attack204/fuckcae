<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wiki.primo.generator.mybatis.plus.cae.mapper.DetailPlanMapper">
	<!--开启二级缓存-->
	<cache/>

	<resultMap id="BaseResultMap" type="wiki.primo.generator.mybatis.plus.cae.entity.DetailPlan">
		<id column="id" property="id" />
		<result column="company_name" property="companyName" />
		<result column="deal_time" property="dealTime" />
		<result column="deal_money" property="dealMoney" />
		<result column="certificate_number" property="certificateNumber" />
		<result column="land_location" property="landLocation" />
		<result column="land_area" property="landArea" />
		<result column="create_time" property="createTime" />
		<result column="creator" property="creator" />
		<result column="update_time" property="updateTime" />
		<result column="updater" property="updater" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        `id`, `company_name` AS `companyName`, `deal_time` AS `dealTime`, `deal_money` AS `dealMoney`, `certificate_number` AS `certificateNumber`, `land_location` AS `landLocation`, `land_area` AS `landArea`, `create_time` AS `createTime`, `creator`, `update_time` AS `updateTime`, `updater`
    </sql>
</mapper>
