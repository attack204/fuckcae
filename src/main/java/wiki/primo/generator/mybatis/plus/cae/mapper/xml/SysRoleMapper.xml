<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wiki.primo.generator.mybatis.plus.cae.mapper.SysRoleMapper">
	<!--开启二级缓存-->
	<cache/>

	<resultMap id="BaseResultMap" type="wiki.primo.generator.mybatis.plus.cae.entity.SysRole">
		<id column="id" property="id" />
		<result column="corp" property="corp" />
		<result column="name" property="name" />
		<result column="status" property="status" />
		<result column="perm_identifier" property="permIdentifier" />
		<result column="role_order" property="roleOrder" />
		<result column="remarks" property="remarks" />
		<result column="create_time" property="createTime" />
		<result column="creator" property="creator" />
		<result column="update_time" property="updateTime" />
		<result column="updater" property="updater" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        `id`, `corp`, `name`, `status`, `perm_identifier` AS `permIdentifier`, `role_order` AS `roleOrder`, `remarks`, `create_time` AS `createTime`, `creator`, `update_time` AS `updateTime`, `updater`
    </sql>
</mapper>
