<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wiki.primo.generator.mybatis.plus.cae.mapper.SysUserMapper">
	<!--开启二级缓存-->
	<cache/>

	<resultMap id="BaseResultMap" type="wiki.primo.generator.mybatis.plus.cae.entity.SysUser">
		<id column="id" property="id" />
		<result column="username" property="username" />
		<result column="corp" property="corp" />
		<result column="mail" property="mail" />
		<result column="phone" property="phone" />
		<result column="sex" property="sex" />
		<result column="password" property="password" />
		<result column="status" property="status" />
		<result column="create_time" property="createTime" />
		<result column="creator" property="creator" />
		<result column="update_time" property="updateTime" />
		<result column="updater" property="updater" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        `id`, `username`, `corp`, `mail`, `phone`, `sex`, `password`, `status`, `create_time` AS `createTime`, `creator`, `update_time` AS `updateTime`, `updater`
    </sql>
</mapper>
